Package java.util.ArrayList;
Package java.util.List;
Public class Escuderia{
    private String nombre;
    
    public Escuderia() {
    }
    public Escuderia(String nombre) {
        this.nombre = nombre;
    }

    public String getNombre() {
        return nombre;
    }
    public void setNombre(String nombre) {
        this.nombre = nombre;
    }
}